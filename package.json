    "test": "echo \"Error: no test specified\" && exit 1",
    "gen-playwright-config": "nx g @nx/playwright:configuration --project=playwright --directory=sandbox/tests --webServerCommand=\"nx serve playwright -p 4200\" --webServerAddress=\"http://localhost:4200\"",
    "gen-playwright": "nx g @nx/playwright tests --directory=sandbox/tests --webServerCommand=\"nx serve playwright -p 4200\" --webServerAddress=\"http://localhost:4200\"",
    "gen-bun": "echo 'Enter Project Name' && read name && nx g @nx-bun/nx:application --name=$name --directory=sandbox/$name",
    "gitPush": "git add . && echo 'Enter Commit Message' && read message && git commit -m \"$message\" && git push",
    "gen-box": "echo 'Enter Project Name' && read name && nx g @nx-bun/nx:create $name --template cindersax/htmx-bun-chat --directory=sandbox/$name --importPath=@mono-agent/$name"

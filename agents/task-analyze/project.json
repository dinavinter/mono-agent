{
  "name": "task-analyze",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "task-analyze/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/task-analyze",
        "main": "task-analyze/src/index.ts",
        "tsConfig": "task-analyze/tsconfig.lib.json",
        "assets": ["task-analyze/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "task-analyze/**/*.ts",
          "task-analyze/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../coverage/task-analyze"
      }
    }
  },
  "tags": []
}
